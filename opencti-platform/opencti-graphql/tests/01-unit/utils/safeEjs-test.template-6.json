      "contentType": "application/vnd.microsoft.card.thumbnail",
      "content": {
        "subtitle": "Content in subtitle : <%= data[0].instance.content %>",
        "text": "Content everywhere !!! <%= data[0].instance.content %>",
        "title": "Content in title : <%= data[0].instance.content %>",
