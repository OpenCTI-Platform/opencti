{
  "name": "opencti-graphql",
  "version": "1.0.0",
  "main": "src/server.js",
  "author": "Luatix",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "start": "yarn schema --env=development && yarn migrate --env=development && babel-node -r node_modules/dotenv/config ./src/server.js --e development",
    "build": "babel --out-dir ./dist ./src",
    "serv": "node dist/server.js",
    "schema": "babel-node ./src/schema/schema-init.js",
    "migrate": "babel-node ./src/database/migration.js",
    "migrate:add": "migrate create",
    "lint": "eslint .",
    "test": "yarn build && yarn schema --e test && jest --coverage --e test",
    "clean": "babel-node ./src/database/clean.js"
  },
  "bin": "./dist/server.js",
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "config/**/*",
      "public/**/*"
    ]
  },
  "engines": {
    "node": ">= 10.* || 8.*"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "@godaddy/terminus": "^4.1.2",
    "amqplib": "^0.5.3",
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.5.0",
    "await": "^0.2.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bytes": "^3.1.0",
    "compression": "^1.7.4",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.0.0",
    "elasticsearch": "^16.0.0",
    "express": "^4.17.1",
    "grakn-client": "^1.5.3",
    "graphql": "^14.3.1",
    "graphql-constraint-directive": "^1.4.0",
    "graphql-import": "^0.7.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-redis-subscriptions": "^2.1.0",
    "graphql-relay": "^0.6.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^4.0.4",
    "helmet": "^3.18.0",
    "ioredis": "^4.9.5",
    "iterall": "^1.2.2",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.5.8",
    "migrate": "^1.6.2",
    "moment": "^2.24.0",
    "nconf": "^0.10.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "ramda": "^0.26.1",
    "rxjs": "^6.5.2",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/node": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "apollo-server-testing": "^2.5.0",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "pkg": "^4.4.0",
    "prettier": "^1.17.1"
  },
  "jest": {
    "testRegex": "/tests/.*/.*-test.js$"
  }
}
